{
  "extends": [
    "./tslint-base.json",
    "./tslint-vendor.json"
  ],
  "rules": {
    "eofline": false,
    "no-trailing-whitespace": false,
    "no-inferrable-types": false,
    "typedef": [
      true,
      "parameter",
      "property-declaration",
      "variable-declaration",
      "member-variable-declaration"
    ],
    "no-any": true,
    "no-duplicate-super": true,
    "no-empty": [
      true,
      "allow-empty-catch"
    ],
    "no-string-literal": false,
    "no-for-in-array": false,
    "no-console": false,
    "array-type": [
      true,
      "array"
    ],
    "ban": [
      true,
      "eval"
    ]
  }
}