{
  "name": "tslint-ssen-rules",
  "version": "1.1.0",
  "description": "SSen's tslint rules.",
  "main": "tslint-config.json",
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rm -rf rules/*",
    "test": "jest",
    "test-files": "tslint 'testFiles/**/*.ts'"
  },
  "author": "Lee Seoyeon",
  "license": "MIT",
  "dependencies": {
    "tslint-config-prettier": "^1.17.0",
    "tslint-consistent-codestyle": "^1.15.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-react": "^3.6.0",
    "tslint-react-hooks": "^1.1.0"
  },
  "devDependencies": {
    "@types/jest": "^23.3.13",
    "@types/node": "^10.12.20",
    "jest": "^24.0.0",
    "mobx": "^4.5.0",
    "mobx-react": "^5.4.3",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.1",
    "typescript": "^3.2.4"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testMatch": [
      "**/__tests__/**/*.ts?(x)",
      "**/?(*.)(spec|test).ts?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  }
}
